package ui;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import javax.swing.JButton;

public class Knopje extends JButton implements ActionListener{
	private String name;
	public Knopje(String name){
		this.name = name;
		this.addActionListener(this);
	}

	@Override
	public void actionPerformed(ActionEvent e) {
		GraphicalUserInterface.getTextFieldContent();
	}
}
